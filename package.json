{
  "name": "subscription-graphql",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .env.development vite",
    "generate:schema:dev": "npx apollo schema:download --endpoint=http://localhost:4200/graphql graphql-schema.json",
    "generate:types": "npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=./src/graphql/**/* --tagName=gql --addTypename --globalTypesFile=src/types/graphql-types.ts types",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@mui/styled-engine-sc": "^5.11.0",
    "@types/node": "^18.13.0",
    "apollo": "^2.34.0",
    "graphql": "^15.6.1",
    "graphql-ws": "^5.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "env-cmd": "^10.1.0",
    "react-error-overlay": "6.0.9",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
