{
  "name": "subscription-graphql",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "dev": "env-cmd -f .env.development vite",
    "codegen": "graphql-codegen-esm --config codegen.ts",
    "generate:schema:dev": "npx apollo schema:download  --endpoint=http://localhost:4200/graphql graphql-schema.json",
    "generate:types": "npx apollo client:codegen --localSchemaFile=graphql-schema.json --target=typescript  --includes=D:/spyle/subscription-graphql/src/graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/types/graphql-types.ts types",
    "build": "env-cmd -f .env.production tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@mui/styled-engine-sc": "^5.11.0",
    "@mui/styles": "^5.11.9",
    "@paypal/react-paypal-js": "^7.8.2",
    "@types/apollo-upload-client": "17.0.1",
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^18.13.0",
    "@types/simple-peer": "^9.11.8",
    "apollo": "^2.34.0",
    "apollo-upload-client": "17.0.0",
    "cross-fetch": "^3.1.5",
    "crypto-js": "^4.2.0",
    "framer-motion": "^10.11.2",
    "graphql": "^15.6.1",
    "graphql-ws": "^5.11.3",
    "moment": "^2.29.4",
    "notistack": "^2.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.2",
    "react-push-notification": "^1.5.3",
    "react-router-dom": "^6.8.1",
    "react-spinners": "^0.13.8",
    "react-waypoint": "^10.3.0",
    "simple-peer": "^9.11.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-codegen/import-types-preset": "^2.2.6",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "env-cmd": "^10.1.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "react-error-overlay": "6.0.9",
    "typescript": "^4.9.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  },
  "engines": {
    "node": "^16.15.1",
    "npm": "^8.11.0"
  }
}
